			"\t<!-- <link rel=\"icon\" href=\"/favicon.svg\" type=\"image/svg+xml\"> -->",
			"\t<!-- <link rel=\"apple-touch-icon\" href=\"/apple-touch-icon.png\"> -->",
			"</head>",
			"",
			"<body>",
			"\t<nav class=\"headering\">",
			"\t\t<ul>",
			"\t\t\t<li><a href=\"/\">Home</a></li>",
			"\t\t</ul>",
			"\t</nav>",
